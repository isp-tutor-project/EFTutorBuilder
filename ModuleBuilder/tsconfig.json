{
  "include": [
      "src/**/*"
  ],
  "compilerOptions": {
      "module": "commonjs",
      "target": "es2016",
      "esModuleInterop": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "src/types/*"
          ]
      }
  }
}